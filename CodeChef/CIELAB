import java.util.*;
class CIELAB
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int d=a-b;
        int d1=d+1;
        if((d%10)==9)
        {
        d1=d-2;
        }
    System.out.println(d1);
}


}
